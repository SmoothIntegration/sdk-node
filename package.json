{
    "private": false,
    "name": "@smooth-integration/sdk-node",
    "packageManager": "pnpm@9.11.0",
    "version": "0.2.9",
    "author": "Smooth Integration",
    "license": "MIT",
    "homepage": "https://smooth-integration.com/",
    "repository": {
        "type": "git",
        "url": "git://github.com/SmoothIntegration/sdk-node.git"
    },
    "files": [
        "package.json",
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "tsup --watch",
        "test": "jest --config jest.config.ts",
        "build": "tsup",
        "verify": "tsc --noEmit",
        "lint": "eslint .",
        "format": "prettier --write .",
        "ci": "pnpm install --frozen-lockfile && pnpm lint",
        "pre-commit": "lint-staged && pnpm verify",
        "prepare": "husky"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@trivago/prettier-plugin-sort-imports": "^5.2.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.5",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-astro": "^1.3.1",
        "husky": "9.1.7",
        "jest": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^15.3.0",
        "prettier": "^3.4.2",
        "prettier-plugin-astro": "^0.14.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.5",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.19.1"
    }
}
